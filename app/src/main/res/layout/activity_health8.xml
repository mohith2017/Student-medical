<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Health8">

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="25dp"
        android:text="Dental examination"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="31dp" />

    <EditText
        android:id="@+id/editText34"
        android:layout_width="275dp"
        android:layout_height="79dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/editText36"
        android:layout_marginTop="90dp"
        android:ems="10"
        android:hint="Complaints"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editText35"
        android:layout_width="272dp"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/editText34"
        android:layout_marginTop="184dp"
        android:ems="10"
        android:hint="Examination"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editText36"
        android:layout_width="271dp"
        android:layout_height="106dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="166dp"
        android:ems="10"
        android:hint="Diagnosis"
        android:inputType="textPersonName" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:text="Next" />

    <Button
        android:id="@+id/button21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:text="Back" />
</RelativeLayout>